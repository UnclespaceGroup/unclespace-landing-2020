  "scripts": {
    "start": "node scripts/start.js",
    "start-ssr": "cross-env REACT_APP_SSR=true node scripts/start.js",
    "serve": "node ./build/server/index.js",
    "build": "node scripts/build.js",
    "build-server": "node scripts/build-server.js",
    "build-ssr": "cross-env-shell REACT_APP_SSR=true \"npm run build && npm run build-server\"",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "cd src/framework && ../../node_modules/.bin/start-storybook -p 9001 -c .storybook -s ./public",
    "analyze": "source-map-explorer build/client/static/js/17.*"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "/Users/sedlukha/WebstormProjects/anexb2c/node_modules/jest-watch-typeahead/filename.js",
      "/Users/sedlukha/WebstormProjects/anexb2c/node_modules/jest-watch-typeahead/testname.js"
    ]
